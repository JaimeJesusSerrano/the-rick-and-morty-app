{
  "name": "the-rick-and-morty-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "~4.9.0",
    "@material-ui/icons": "~4.9.0",
    "@react-corekit/use-idle": "^1.1.0",
    "axios": "~0.19.0",
    "react": "~16.13.0",
    "react-dom": "~16.13.0",
    "react-infinite-scroller": "~1.2.0",
    "react-lazy-load-image-component": "~1.4.0",
    "react-redux": "~7.2.0",
    "react-router-dom": "~5.1.0",
    "react-scripts": "3.4.1",
    "recharts": "~1.8.0",
    "redux": "~4.0.0",
    "redux-saga": "~1.1.0",
    "styled-components": "~5.0.0",
    "uuid": "~7.0.0"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test --coverage",
    "test:update": "rescripts test --coverage --updateSnapshot",
    "eject": "rescripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "~Api(.*)$": "<rootDir>/src/services/api$1",
      "~Assets(.*)$": "<rootDir>/src/assets$1",
      "~Components(.*)$": "<rootDir>/src/components$1",
      "~Hooks(.*)$": "<rootDir>/src/hooks$1",
      "~Routes(.*)$": "<rootDir>/src/routes$1",
      "~Screens(.*)$": "<rootDir>/src/screens$1",
      "~Store(.*)$": "<rootDir>/src/store$1",
      "~Theme(.*)$": "<rootDir>/src/theme$1",
      "~Test(.*)$": "<rootDir>/src/__test__$1",
      "~Utils(.*)$": "<rootDir>/src/utils$1"
    },
    "resetMocks": true,
    "resetModules": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "~8.3.0",
    "@commitlint/config-conventional": "~8.3.0",
    "@rescripts/cli": "~0.0.13",
    "@rescripts/rescript-use-babel-config": "~0.0.9",
    "@rescripts/rescript-use-eslint-config": "~0.0.10",
    "@svgr/webpack": "~5.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-lazy-load-image-component": "^1.3.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.2",
    "@types/recharts": "^1.8.9",
    "@types/redux-mock-store": "^1.0.2",
    "@types/selenium-webdriver": "4.0.*",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-plugin-module-resolver": "~4.0.0",
    "chromedriver": "~80.0.0",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.0",
    "enzyme-to-json": "~3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "geckodriver": "~1.19.0",
    "lint-staged": "~10.0.0",
    "prettier": "~1.19.0",
    "react-test-renderer": "~16.13.0",
    "redux-mock-store": "~1.5.0",
    "selenium-webdriver": "~3.6.0",
    "typescript": "~3.7.2"
  }
}
